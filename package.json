    "npm": "1.3.2"
  },
  "homepage": "https://github.com/fraserxu/qiniu-image-gallery",
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.10.1",
    "compression": "^1.3.0",
    "dotenv": "^0.4.0",
    "envify": "^3.2.0",
    "errorhandler": "^1.3.2",
    "express": "^4.10.6",
    "express-session": "^1.9.3",
    "jade": "^1.8.2",
    "morgan": "^1.5.1",
